# Provider padrão (opcional)
DEFAULT_AI_PROVIDER=openai

# OpenAI
OPENAI_API_KEY=sk-sua-chave-openai-aqui

# Anthropic (opcional)
ANTHROPIC_API_KEY=sua-chave-anthropic-aqui

# Configurações de modelo padrão
DEFAULT_MODEL=gpt-3.5-turbo
DEFAULT_MAX_TOKENS=1000
DEFAULT_TEMPERATURE=0.7

# Configurações de timeout
REQUEST_TIMEOUT=30

# Configurações do Backend
BACKEND_URL=http://localhost:3000

# Configurações SQS para Consumer
AWS_ENDPOINT_URL=http://localhost:4566
AWS_ACCESS_KEY_ID=test
AWS_SECRET_ACCESS_KEY=test
AWS_REGION=us-east-1

# Filas SQS
APPLICATIONS_SQS_QUEUE_NAME=applications-queue
APPLICATIONS_AI_SCORE_SQS_QUEUE_NAME=applications-ai-score-queue
QUESTION_RESPONSES_SQS_QUEUE_NAME=question-responses-queue

# Configurações SQS
SQS_MAX_RETRIES=3
SQS_WAIT_TIME=20
SQS_MAX_MESSAGES=10

# Configurações do Banco de Dados
DB_HOST=postgres
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=postgres
DB_NAME=cognitive_ats

# Configurações de Processamento
DOWNLOAD_TIMEOUT=30
TEMP_FILE_SUFFIX=.pdf

# Configurações de Logging
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s

# Configurações adicionais (opcional)
# DEBUG=true